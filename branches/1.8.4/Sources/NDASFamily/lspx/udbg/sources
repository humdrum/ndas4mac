TARGETNAME=liblspx_udbg
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=liblspx_udbg

INCLUDES=$(NDAS_INC_PATH)\lspx;$(NDAS_INC_PATH)

!IF $(FREEBUILD)
MSC_OPTIMIZATION=/Ox
!ELSE
!ENDIF

# publish lib files to the project root
PASS1_PUBLISH=\
	{$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

MSC_WARNING_LEVEL=-W3 -Wp64 -we4100 -we4189

C_DEFINES=$(C_DEFINES) -DLSPIMP_DEBUG_UMODE

SOURCES=\
	..\lsp_hash.c \
	..\lsp.c \
	..\lsp_util.c \
	..\lsp_debug.c
